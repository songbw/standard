<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiannei.dao.BZJTreeDAO">

    <resultMap id="bzjTreeResultMap" type="com.jiannei.bean.BZJTreeBean">
        <id property="id" column="id" />
        <result property="name" column="name"/>
        <result property="count" column="count"/>
        <result property="url" column="url"/>
        <result property="parentId" column="parentId"/>
        <result property="kinship" column="kinship"/>
    </resultMap>

    <select id="selectByUrl" resultMap="bzjTreeResultMap">
        select * from bzj_tree where url != 'null';
    </select>

    <select id="selectByParentId"  resultMap="bzjTreeResultMap" parameterType="Long">
        SELECT * FROM bzj_tree WHERE parentId = #{id}
    </select>
</mapper>